{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "eslint.autoFixOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.enable": true,
  "cSpell.words": [
    "Xiao",
    "wangwang",
    "xiaoma"
  ],
  "compile-hero.disable-compile-files-on-did-save-code": true,
}
